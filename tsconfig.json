{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"strict": true,
		"target": "ES6",
		"module": "node16",
		"moduleResolution": "node16",
		"jsx": "react-native",
		"lib": ["dom", "esnext"],
		"noEmit": true,
		"allowSyntheticDefaultImports": true,
		"skipLibCheck": true,
		"resolveJsonModule": true,
		"baseUrl": ".",
		"paths": {
			"@/*": ["./src/*"],
			"@/compnents/*": ["./src/components/*"],
			"@/assets/*": ["./assets/*"],
			"@/utils/*": ["./src/utils/*"],
			"@/types/*": ["./src/types/*"],
			"@/styles/*": ["./src/styles/*"]
		}
	},
	"include": ["**/*.ts", "**/*.tsx"],
	"exclude": ["node_modules"]
}
