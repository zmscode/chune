{
	"expo": {
		"name": "chune",
		"slug": "chune",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"userInterfaceStyle": "dark",
		"scheme": "chune",
		"newArchEnabled": true,
		"splash": {
			"image": "./assets/splash-icon.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"ios": {
			"supportsTablet": true,
			"bundleIdentifier": "com.zms.chune",
			"infoPlist": {
				"UIBackgroundModes": ["audio"]
			}
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.png",
				"backgroundColor": "#ffffff"
			},
			"package": "com.zms.chune",
			"edgeToEdgeEnabled": true,
			"permissions": [
				"android.permission.RECORD_AUDIO",
				"android.permission.MODIFY_AUDIO_SETTINGS"
			]
		},
		"web": {
			"favicon": "./assets/favicon.png",
			"bundler": "metro"
		},
		"plugins": [
			"expo-router",
			"expo-build-properties",
			"expo-asset",
			[
				"expo-audio",
				{
					"microphonePermission": "Allow Chune to access your microphone."
				}
			],
			"expo-font"
		],
		"owner": "zmscode",
		"extra": {
			"router": {},
			"eas": {
				"projectId": "d6fc1667-cb08-43a6-adaa-a2391e429475"
			}
		}
	}
}
